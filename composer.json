{
    "name": "asseco-voice/laravel-attachments",
    "description": "Laravel support for attachments",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.4",
        "mockery/mockery": "^1.3.1",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Asseco\\Attachments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\Attachments\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\Attachments\\AttachmentsServiceProvider"
            ]
        }
    }
}
